# Starter Plan Configuration Example
# Copy this file to terraform.tfvars and customize

# AWS Configuration
aws_region = "us-east-1"

# Domain Configuration (optional)
domain_name = "starter.mypaas.com"

# Database Configuration
rds_username = "paasadmin"
rds_password = "ChangeThisSecurePassword123!"

# Instance Configuration
master_instance_type = "t4g.medium"
worker_instance_type = "t4g.medium"

# Auto Scaling
worker_desired_capacity = 2
worker_min_size = 1
worker_max_size = 10

# S3 Configuration
s3_code_bucket_name = ""

# SSH Key
key_pair_name = "your-key-pair-name"
